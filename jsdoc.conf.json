{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [ "./src", "README.md" ],
    "exclude": [ "./src/xmlParser.js", "./src/helperFunctions.js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": "true"
  }

}

